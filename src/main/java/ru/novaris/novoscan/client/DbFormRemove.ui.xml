<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
	<g:DockLayoutPanel unit="EM" ui:field="panel" width="200"
		height="100">
		<g:north size="4">
			<g:HTMLPanel>

				<g:Label ui:field="removeInfo" text="Remove?" horizontalAlignment="ALIGN_CENTER" />
			</g:HTMLPanel>
		</g:north>
		<g:center size="4">
			<g:HTMLPanel>
				<table>

					<tr>
						<td>
							<g:PushButton text="Cancel" ui:field="cancel"
								addStyleNames="button" width="60px"/>
						</td>
						<td width="60px"/>
						<td>
							<g:PushButton text="Apply" ui:field="apply"
								addStyleNames="button" width="60px"/>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder>