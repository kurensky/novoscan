<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="constants" type="ru.novaris.novoscan.client.resources.Constants" />
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="panel"
			verticalAlignment="middle" horizontalAlignment="center">
			<g:Label addStyleNames="gwt-Login-Head" ui:field="head" />
			<g:SuggestBox ui:field="username" />
			<g:PasswordTextBox ui:field="password" />
			<g:HorizontalPanel ui:field="checkPanel">
				<g:CheckBox ui:field="remember" />
				<g:Label addStyleNames="gwt-Login-Label" text="{constants.Remember}" />
			</g:HorizontalPanel>
			<g:Button ui:field="submit" text="{constants.Login}" />
			<g:Label ui:field="errorLabel" />
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 